{
  "name": "@theprodev/nest-example-app",
  "private": true,
  "description": "Example NestJS Application Utilizing Shared Nest Modules",
  "scripts": {
    "dev": "NODE_ENV=development nest start --watch",
    "dev:debug": "NODE_ENV=development nest start --debug 0.0.0.0:9229 --watch",
    "format": "prettier \"./**/*.{js,ts,tsx,json,md}\" --check",
    "format:fix": "prettier --write \"./**/*.{js,ts,tsx,json,md}\"",
    "lint": "eslint \"./src/**/*.{ts,js,tsx,json}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,js,tsx,json}\" --fix"
  },
  "prettier": "@theprodev/prettier-config",
  "dependencies": {
    "@nestjs/common": "10.4.4",
    "@nestjs/core": "10.4.4",
    "@nestjs/platform-express": "10.4.4",
    "@theprodev/nest-http": "workspace:*",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2"
  }
}
